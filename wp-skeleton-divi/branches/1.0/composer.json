{
  "name": "pollen-solutions/wp-skeleton-divi",
  "description": "Pollen Wordpress + Divi Skeleton - Standard Installation.",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-skeleton-divi/",
  "keywords": [
    "pollen solutions",
    "skeleton",
    "wordpress",
    "standard",
    "Divi"
  ],
  "time": "2021-07-13",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "composer/installers": "~1.0",
    "pollen-solutions/wp-theme-divi": "^1.0",
    "pollen-solutions/wp-kernel": "^1.0",
    "wpackagist-theme/twentytwentyone": "^1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wp-content/mu-plugins/{$name}": [
        "type:wordpress-muplugin"
      ],
      "wp-content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "vendor/bin/wp-salt >> .env"
    ]
  }
}
